
@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Address</th>
        <th>Date Joined</th>
    </tr>
    <tr>
        <td>@Model.ID</td>
        <td>@Model.Name</td>
        <td>@Model.Email</td>
        <td>@Model.Phone</td>
        <td>@Model.Address</td>
        <td>@Model.DateJoined</td>
    </tr>
</table>

